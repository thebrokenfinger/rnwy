{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Rnwy",
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["es6"],
    "allowJs": true,
    "outDir": "build",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "resolveJsonModule": true
  },
  "ts-node": {
    "files": true
  }
}
